{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "${default}",
                "lib/nrf5_sdk**"
            ],
            "defines": [
                "BOARD_PCA10056",
                "BSP_DEFINES_ONLY",
                "CONFIG_GPIO_AS_PINRESET",
                "ESB_PRESENT",
                "FLOAT_ABI_HARD",
                "NRF52840_XXAA",
                "ENABLE_SWO"
            ],
            "compilerPath": "${env:HOME}/opt/arm-none-eabi/bin/arm-none-eabi-gcc",
            "cStandard": "gnu11",
            "cppStandard": "gnu++14",
            "intelliSenseMode": "gcc-arm",
            "browse": {"path": ["lib/nrf5_sdk**"]}
        }
    ],
    "version": 4
}